  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "node node_modules/jest/bin/jest.js --ci",
    "test-debug": "node node_modules/jest/bin/jest.js -u --watchAll --collectCoverage=false -i --logHeapUsage --detectOpenHandles --detectLeaks --debug",
